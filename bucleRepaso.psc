Proceso bucleRepaso
	
	//REPETIR
	Definir i Como Entero;
	i<-1;
	
	Repetir
		Escribir i;
		i <- i + 1;
	Hasta Que i>10
	
	//MIENTRAS
	Definir j Como Entero;
	j<-1;
	Mientras j<=10 Hacer
		Escribir j;
		j <- j+1;
	FinMientras
	
	//PARA
	Definir k Como Entero;
	Para k<-1 Hasta 10 Con Paso 1 Hacer
		Escribir k;
	FinPara
	
FinProceso
